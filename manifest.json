{
  "manifest_version": 2,

  "name": "Chrome AWS API Debugger",
  "description": "This APP is for AWS API debug",
  "version": "0.1.1",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": {
    "16": "AWScloud-16.png",
    "128": "AWScloud-128.png"
  },
  "permissions": [
    "http://*/", "https://*/", "storage", {
      "fileSystem": ["write"]
    }
  ]
}